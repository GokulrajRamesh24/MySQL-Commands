USE SCHOOL;
CREATE TABLE STUDENTS1(
Student_ID INT Primary Key,
First_Name VARCHAR(50),
Last_Name VARCHAR(50),
Course_ID INT);
CREATE TABLE COURSES(
Course_ID INT Primary key,
Course_Name VARCHAR(50),
Department VARCHAR(50),
Credits INT);

INSERT INTO STUDENTS1 VALUES
(1,'Alice','Johnson',101),
(2,'Bob','Smith',102),
(3,'Charlie','Brown',103),
(4,'David','Williams',101),
(5,'Eve','Taylor',NULL),
(6,'Frank','Moore',104);

INSERT INTO COURSES VALUES
(101,'Mathematics','Science',4),
(102,'Physics','Science',3),
(103,"History",'Arts',2),
(104,'Computer Science','Technology',5),
(105,'Chemistry','Science',3);

SELECT FIRST_NAME,COURSE_NAME FROM STUDENTS1 NATURAL JOIN COURSES;
SELECT FIRST_NAME,LAST_NAME,COURSE_NAME,DEPARTMENT FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT FIRST_NAME,LAST_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID UNION SELECT FIRST_NAME,LAST_NAME,COURSE_NAME FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(First_Name,' ',Last_Name) as FULL_NAME,CREDITS FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE CREDITS>3;
SELECT FIRST_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE DEPARTMENT='SCIENCE';
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID ORDER BY FULL_NAME;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME,CREDITS FROM STUDENTS1  JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE COURSES.CREDITS=3;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE DEPARTMENT='ARTS';
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID; 
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,CREDITS FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE COURSE_NAME IS NULL;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME,CREDITS FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID ORDER BY COURSES.CREDITS;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME,DEPARTMENT FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,DEPARTMENT FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE STUDENTS1.COURSE_ID IS NULL;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE COURSE_NAME='COMPUTER SCIENCE';
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,IFNULL(COURSE_NAME,'NOT ASSIGNED') AS COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,CREDITS FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE CREDITS>=4;
SELECT * FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID ORDER BY STUDENT_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID UNION SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT COURSE_NAME FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE STUDENTS1.COURSE_ID IS NULL;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME,CREDITS FROM STUDENTS1 INNER JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME,DEPARTMENT,COURSE_NAME FROM STUDENTS1 INNER JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE DEPARTMENT='SCIENCE'; 
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID ORDER BY COURSE_NAME;
SELECT * FROM STUDENTS1 LEFT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID UNION SELECT * FROM STUDENTS1 RIGHT JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 CROSS JOIN COURSES;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 INNER JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID HAVING FULL_NAME LIKE('%A%');
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,CREDITS FROM STUDENTS1 INNER JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE CREDITS<4;
SELECT CONCAT(FIRST_NAME,' ',LAST_NAME) AS FULL_NAME,COURSE_NAME FROM STUDENTS1 INNER JOIN COURSES ON STUDENTS1.COURSE_ID=COURSES.COURSE_ID WHERE COURSE_NAME IN ('MATHEMATICS','HISTORY'); 
